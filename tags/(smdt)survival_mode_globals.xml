<plugin>
	<_field_real name="respawn time#NO: Use game_engine_settings for this" />
	<_field_tag_reference name="survival mode text" validtags="unic" />
	<_field_tag_reference name="countdown sound" validtags="scmb, sndo, snd!" />
	<_field_tag_reference name="respawn sound" validtags="scmb, sndo, snd!" />
	<_field_tag_reference name="co-op spawning globals" validtags="coop" />
	<_field_block name="wave templates" max="128" srcguid="8d5a4d84-f43e-de4a-ab29-718311a7acc1" srcName="survival_mode_wave_templates">
		<_field_string_id name="name^" />
		<_field_tag_reference name="wave template" validtags="wave" />
	</_field_block>
	<_field_block name="state responses" max="32" srcguid="8a81a479-a8cc-1f4a-96b1-b1d43f614f45" srcName="game_engine_status_response_block">
		<_field_word_flags name="flags">
			<item Name="unused" />
		</_field_word_flags>
		<_field_pad name="FAW" length="2" />
		<_field_short_enum name="state^">
			<item Name="waiting for space to clear" />
			<item Name="observing" />
			<item Name="respawning soon" />
			<item Name="sitting out" />
			<item Name="out of lives" />
			<item Name="playing (winning)" />
			<item Name="playing (tied)" />
			<item Name="playing (losing)" />
			<item Name="game over (won)" />
			<item Name="game over (tied)" />
			<item Name="game over (lost)" />
			<item Name="game over (you lost, but game tied)" />
			<item Name="you have flag" />
			<item Name="enemy has flag" />
			<item Name="flag not home" />
			<item Name="carrying oddball" />
			<item Name="you are juggy" />
			<item Name="you control hill" />
			<item Name="switching sides soon" />
			<item Name="player recently started" />
			<item Name="you have bomb" />
			<item Name="flag contested" />
			<item Name="bomb contested" />
			<item Name="limited lives left (multiple)" />
			<item Name="limited lives left (single)" />
			<item Name="limited lives left (final)" />
			<item Name="playing (winning, unlimited)" />
			<item Name="playing (tied, unlimited)" />
			<item Name="playing (losing, unlimited)" />
			<item Name="waiting to spawn" />
			<item Name="waiting for game start" />
			<item Name="blank" />
		</_field_short_enum>
		<_field_pad name="BNYFIDDGX" length="2" />
		<_field_string_id name="ffa message" />
		<_field_string_id name="team message" />
		<_field_tag_reference name="unused!" />
		<_field_pad name="GTL" length="4" />
	</_field_block>
	<_field_block name="team colors" max="32" srcguid="a866987e-8fde-6f42-9f08-988dfed37a7c" srcName="multiplayer_color_block">
		<_field_real_rgb_color name="color" />
	</_field_block>
</plugin>